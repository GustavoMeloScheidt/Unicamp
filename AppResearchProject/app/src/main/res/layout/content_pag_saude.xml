        android:background="@color/white"
        android:text="SANGUÍNEA"
        app:layout_constraintBottom_toBottomOf="@+id/pressaoXML"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.545"
